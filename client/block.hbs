<ul class="nav nav-tabs">
  {{#each details.assetsDisplayTabs as |assetsDisplayTab|}}
    <li class="nav-item">
      <a
        {{action "changeTab" assetsDisplayTab}}
        class="nav-link
          {{if (or (eq activeTab assetsDisplayTab) (not activeTab)) 'active'}}"
        href="#"
      >
        {{assetsDisplayTab}}
      </a>
    </li>
  {{/each}}
</ul>
{{#each details.assetsDisplayTabs as |assetsDisplayTab|}}
  {{#if (eq activeTab assetsDisplayTab)}}
    {{#each (get details.assetsResults assetsDisplayTab) as |asset index|}}
      <div class="tab-results">
        {{#if (get expandableTitleStates (concat assetsDisplayTab index))}}
          <a
            href="#"
            class="expandable-title"
            {{on "click" (action "toggleExpandableTitle" assetsDisplayTab index)}}
          >
            <span class="text">{{asset.entity.displayName}}</span>
            {{fa-icon "angle-up"}}
          </a>
          <div class="details">
            <div class="p-link">
              <a
                href="https://{{block.userOptions.instanceId}}.apps.us.jupiterone.io/assets/inventory/{{asset.id}}"
              >
                View in JupiterOne
                {{fa-icon icon="external-link-square" class="external-link-icon"}}
              </a>
            </div>
            <div>
              <span class="p-key">_Type: </span>
              <span class="p-value">{{join ", " asset.entity._type}}</span>
            </div>
            {{#if asset.properties.tagAccountName}}
              <div>
                <span class="p-key">Tag Account Name: </span>
                <span class="p-value">{{asset.properties.tagAccountName}}</span>
              </div>
            {{/if}}
            {{#if asset.entity._integrationClass}}
              <div>
                <span class="p-key">Integration Classes: </span>
                <span class="p-value">{{join ", " asset.entity._integrationClass}}</span>
              </div>
            {{/if}}
            {{#each-in asset.properties as |label content|}}
              {{#if (not (includes label excludedProperties))}}
                <div>
                  <span class="p-key">{{titleize (humanize (dasherize label))}}: </span>
                  <span class="p-value">{{content}}</span>
                </div>
              {{/if}}
            {{/each-in}}
          </div>
        {{else}}
          <a
            href="#"
            class="expandable-title"
            {{on "click" (action "toggleExpandableTitle" assetsDisplayTab index)}}
          >
            <span class="text">{{asset.entity.displayName}}</span>
            {{fa-icon "angle-down"}}
          </a>
          <div class="p-link">
            <a
              href="https://{{block.userOptions.instanceId}}.apps.us.jupiterone.io/assets/inventory/{{asset.id}}"
            >
              View in JupiterOne
              {{fa-icon icon="external-link-square" class="external-link-icon"}}
            </a>
          </div>
        {{/if}}
      </div>
    {{/each}}
  {{/if}}
{{/each}}

{{!-- {{#if details}}
  <div class='asset-header'>
    <h1 class='display-name'>{{fa-icon 'server' fixedWidth=true}}
      {{details.entity.displayName}}</h1>
    <div class='asset-info'>
      <span class='type'>{{details.entity._type}}</span>
      <span class='class'>
        <span class='class-border'>{{details.entity._class}}</span>
      </span>
    </div>
  </div>
  <hr class='separator' />
  <h1 class='p-title'>{{fa-icon 'info' fixedWidth=true}} Properties:</h1>

  {{! IP ASSET}}
  {{#if (or (eq entity.type 'IPv4') (eq entity.type 'IPv6') (eq entity.type 'IP'))}}
    {{#if details.entity.displayName}}
      <div>
        <span class='p-key'>Display Name:</span>
        <span class='p-value'>{{details.entity.displayName}}</span>
      </div>
    {{/if}}
    {{#if details.properties.ipAddress}}
      <div>
        <span class='p-key'>IP Address:</span>
        <span class='p-value'>{{details.properties.ipAddress}}</span>
      </div>
    {{/if}}
    {{#if details.properties.publicIpAddress}}
      <div>
        <span class='p-key'>Public IP Address:</span>
        <span class='p-value'>{{details.properties.publicIpAddress}}</span>
      </div>
    {{/if}}
    {{#if details.properties.validated}}
      <div>
        <span class='p-key'>Validated:</span>
        <span class='p-value'>{{details.properties.validated}}</span>
      </div>
    {{/if}}
  {{/if}}
  {{! END OF ASSET}}

  {{! USER}}
  {{#if (eq entity.type 'custom')}}
    {{#if details}}
      <div class='asset-info'>
        {{#if details.properties.arn}}
          <div>
            <span class='p-key'>ARN:</span>
            <span class='p-value'>{{details.properties.arn}}</span>
          </div>
        {{/if}}
        {{#if details.properties.userId}}
          <div>
            <span class='p-key'>User ID:</span>
            <span class='p-value'>{{details.properties.userId}}</span>
          </div>
        {{/if}}
        {{#if details.properties.username}}
          <div>
            <span class='p-key'>Username:</span>
            <span class='p-value'>{{details.properties.username}}</span>
          </div>
        {{/if}}
        {{#if details.properties.name}}
          <div>
            <span class='p-key'>Name:</span>
            <span class='p-value'>{{details.properties.name}}</span>
          </div>
        {{/if}}
        {{#if details.properties.createdOn}}
          <div>
            <span class='p-key'>Created On:</span>
            <span class='p-value'>{{moment-format
                details.properties.createdOn
                timeZone=timezone
              }}</span>
          </div>
        {{/if}}
        {{#if details.properties.path}}
          <div>
            <span class='p-key'>Path:</span>
            <span class='p-value'>{{details.properties.path}}</span>
          </div>
        {{/if}}
        {{#if details.properties.passwordLastUsed}}
          <div>
            <span class='p-key'>Password Last Used:</span>
            <span class='p-value'>{{moment-format
                details.properties.passwordLastUsed
                timeZone=timezone
              }}</span>
          </div>
        {{/if}}
        {{#if details.properties.webLink}}
          <div>
            <span class='p-key'>Web Link:</span>
            <span class='p-value'>{{details.properties.webLink}}</span>
          </div>
        {{/if}}
        {{#if details.properties.passwordEnabled}}
          <div>
            <span class='p-key'>Password Enabled:</span>
            <span class='p-value'>{{details.properties.passwordEnabled}}</span>
          </div>
        {{/if}}
        {{#if details.properties.passwordLastChanged}}
          <div>
            <span class='p-key'>Password Last Changed:</span>
            <span class='p-value'>{{moment-format
                details.properties.passwordLastChanged
                timeZone=timezone
              }}</span>
          </div>
        {{/if}}
        {{#if details.properties.mfaEnabled}}
          <div>
            <span class='p-key'>MFA Enabled:</span>
            <span class='p-value'>{{details.properties.mfaEnabled}}</span>
          </div>
        {{/if}}
        {{#if details.properties.accessKeyEnabled}}
          <div>
            <span class='p-key'>accessKeyEnabled:</span>
            <span class='p-value'>{{details.properties.accessKeyEnabled}}</span>
          </div>
        {{/if}}
        {{#if details.properties.accessKeyLastUsed}}
          <div>
            <span class='p-key'>accessKeyLastUsed:</span>
            <span class='p-value'>{{moment-format
                details.properties.accessKeyLastUsed
                timeZone=timezone
              }}</span>
          </div>
        {{/if}}
        {{#if details.properties.accountId}}
          <div>
            <span class='p-key'>accountId:</span>
            <span class='p-value'>{{details.properties.accountId}}</span>
          </div>
        {{/if}}
        {{#if details.properties.tag.AccountName}}
          <div>
            <span class='p-key'>tag.AccountName:</span>
            <span class='p-value'>{{details.entity.tag.AccountName}}</span>
          </div>
        {{/if}}
      </div>
    {{/if}}
  {{/if}}
  {{! END OF USER}}
  {{! VULNS}}
  {{#if (or (eq entity.type 'cve') (eq entity.type 'custom'))}}
    {{#if details.properties.severity}}
      <div>
        <span class='p-key'>Severity:</span>
        <span class='p-value'>{{details.properties.severity}}</span>
      </div>
    {{/if}}
    {{#if details.properties.status}}
      <div>
        <span class='p-key'>Status:</span>
        <span class='p-value'>{{details.properties.status}}</span>
      </div>
    {{/if}}
    {{#if details.properties.score}}
      <div>
        <span class='p-key'>Score:</span>
        <span class='p-value'>{{details.properties.score}}</span>
      </div>
    {{/if}}
    {{#if details.properties.exploitability}}
      <div>
        <span class='p-key'>Exploitability:</span>
        <span class='p-value'>{{details.properties.exploitability}}</span>
      </div>
    {{/if}}
    {{#if details.properties.impact}}
      <div>
        <span class='p-key'>Impact:</span>
        <span class='p-value'>{{details.properties.impact}}</span>
      </div>
    {{/if}}
    {{#if details.properties.exploitStatus}}
      <div>
        <span class='p-key'>Exploit Status:</span>
        <span class='p-value'>{{details.properties.exploitStatus}}</span>
      </div>
    {{/if}}
    {{#if details.properties.exploitRating}}
      <div>
        <span class='p-key'>Exploit Rating:</span>
        <span class='p-value'>{{details.properties.exploitRating}}</span>
      </div>
    {{/if}}
    {{#if details.properties.open}}
      <div>
        <span class='p-key'>Open:</span>
        <span class='p-value'>{{details.properties.open}}</span>
      </div>
    {{/if}}
    {{#if details.properties.createdOn}}
      <div>
        <span class='p-key'>Created On:</span>
        <span class='p-value'>{{moment-format
            details.properties.createdOn
            timeZone=timezone
          }}</span>
      </div>
    {{/if}}
    {{#if details.properties.closedOn}}
      <div>
        <span class='p-key'>Closed On:</span>
        <span class='p-value'>{{moment-format
            details.properties.closedOn
            timeZone=timezone
          }}</span>
      </div>
    {{/if}}
    {{#if details.properties.updatedOn}}
      <div>
        <span class='p-key'>Updated On:</span>
        <span class='p-value'>{{moment-format
            details.properties.updatedOn
            timeZone=timezone
          }}</span>
      </div>
    {{/if}}
    {{#if details.properties.publishedOn}}
      <div>
        <span class='p-key'>Published On:</span>
        <span class='p-value'>{{moment-format
            details.properties.publishedOn
            timeZone=timezone
          }}</span>
      </div>
    {{/if}}
    {{#if details.properties.description}}
      <div class='scrollable-block'>
        <span class='p-key'>Description:</span>
        <div class='scrollable-container'>
          <table>
            <tbody>
              {{details.properties.description}}
            </tbody>
          </table>
        </div>
      </div>
    {{/if}}
  {{/if}}
  {{! END OF VULNS}}
{{/if}}

